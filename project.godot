; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="godot-jam-70"
run/main_scene="res://Scenes/main_game.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="folklore"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"protagonist": "res://DialogicCharacters/protagonist.dch",
"voice": "res://DialogicStyle/voice.dch"
}
directories/dtl_directory={
"section_1": "res://Timelines/section_1.dtl"
}
variables={}
glossary/default_case_sensitive=true
layout/style_list=["res://DialogicStyle/protagonist_style.tres", "res://DialogicStyle/voice_style.tres", "res://DialogicStyle/for_ss.tres"]
layout/default_style="res://DialogicStyle/protagonist_style.tres"
text/letter_speed=0.04
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=false
choices/delay=0.2
choices/reveal_delay=0.5
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
history/simple_history_enabled=true
history/full_history_enabled=true
history/visited_event_history_enabled=true
history/save_on_autosave=true
history/save_on_save=true
save/autosave=true
text/split_at_new_lines_as=1
text/split_at_new_lines=true
text/autopauses={}
portraits/default_portrait=""

[display]

window/size/mode=3
window/stretch/mode="viewport"
window/canvas_textures/default_texture_filter=0

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

